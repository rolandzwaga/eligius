{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://eligius.com/schema/get-element-dimensions-operation.json",
  "type": "object",
  "title": "The getElementDimensions schema",
  "description": "Calculates the dimensions for the current element",
  "required": [
    "systemName"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "examples": [
        "123e4567-e89b-12d3-a456-426614174000"
      ],
      "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
    },
    "systemName": {
      "$id": "#/properties/systemName",
      "const": "getElementDimensions"
    },
    "operationData": {
      "$id": "#/properties/operationData",
      "type": "object",
      "title": "The Operationdata Schema",
      "properties": {
        "modifier": {
          "$id": "#/properties/operationData/modifier",
          "type": "string"
        }
      }
    }
  }
}