{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://eligius.com/schema/get-query-params-operation.json",
  "type": "object",
  "title": "The getQueryParams schema",
  "description": "This operation retrieves the current query parameters from the browser's address bar and places\nthem on the returned operation data.",
  "required": [
    "systemName"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "examples": [
        "123e4567-e89b-12d3-a456-426614174000"
      ],
      "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
    },
    "systemName": {
      "$id": "#/properties/systemName",
      "const": "getQueryParams"
    },
    "operationData": {
      "$id": "#/properties/operationData",
      "type": "object",
      "title": "The Operationdata Schema",
      "properties": {
        "defaultValues": {
          "$id": "#/properties/operationData/defaultValues",
          "type": "array"
        }
      }
    }
  }
}