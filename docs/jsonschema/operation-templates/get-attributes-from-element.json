{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://eligius.com/schema/get-attributes-from-element-operation.json",
  "type": "object",
  "title": "The getAttributesFromElement schema",
  "description": "Retrieves the values for the specified attribute names from  the given selected element.",
  "required": ["systemName"],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "examples": ["123e4567-e89b-12d3-a456-426614174000"],
      "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
    },
    "systemName": {
      "$id": "#/properties/systemName",
      "const": "getAttributesFromElement"
    },
    "operationData": {
      "$id": "#/properties/operationData",
      "type": "object",
      "title": "The Operationdata Schema",
      "properties": {
        "attributeNames": {
          "$id": "#/properties/operationData/attributeNames",
          "type": "array"
        }
      }
    }
  }
}
