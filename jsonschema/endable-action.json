{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://eligius.com/schema/init-action.json",
  "type": "object",
  "title": "The Endable Action Schema",
  "required": ["name", "startOperations", "endOperations"],
  "properties": {
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": ["ProgressbarSetup"],
      "pattern": "^(.*)$"
    },
    "startOperations": {
      "$id": "#/properties/startOperations",
      "type": "array",
      "title": "The Startoperations Schema",
      "items": {
        "$ref": "file:operation.json"
      }
    },
    "endOperations": {
      "$id": "#/properties/endOperations",
      "type": "array",
      "title": "The Endoperations Schema",
      "items": {
        "$ref": "file:operation.json"
      }
    }
  }
}
