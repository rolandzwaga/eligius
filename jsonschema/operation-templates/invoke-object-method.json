{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://eligius.com/schema/invoke-object-method-operation.json",
  "type": "object",
  "title": "The invokeObjectMethod schema",
  "description": "Invokes the specified method on the given object with the optional specified arguments",
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "examples": [
        "123e4567-e89b-12d3-a456-426614174000"
      ],
      "pattern": "^[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}$"
    },
    "systemName": {
      "$id": "#/properties/systemName",
      "const": "invokeObjectMethod"
    },
    "operationData": {
      "$id": "#/properties/operationData",
      "type": "object",
      "title": "The Operationdata Schema",
      "required": [
        "instance",
        "methodName"
      ],
      "properties": {
        "instance": {
          "$id": "#/properties/operationData/instance",
          "type": "object"
        },
        "methodName": {
          "$id": "#/properties/operationData/methodName",
          "type": "string"
        },
        "methodArguments": {
          "$id": "#/properties/operationData/methodArguments",
          "type": "array"
        }
      }
    }
  }
}