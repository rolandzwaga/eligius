{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://rolandzwaga.github.io/eligius/jsonschema/mediaplayer-timeline-provider-settings.json",
  "type": "object",
  "title": "The TimelineProviderSettings Schema",
  "required": ["id", "vendor", "systemName"],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "examples": ["123e4567-e89b-12d3-a456-426614174000"],
      "pattern": "^[0-9a-fA-F]{8}\b-[0-9a-fA-F]{4}\b-[0-9a-fA-F]{4}\b-[0-9a-fA-F]{4}\b-[0-9a-fA-F]{12}$"
    },
    "vendor": {
      "$id": "#/properties/vendor",
      "type": "string",
      "title": "The Vendor Schema",
      "examples": ["jwplayer"]
    },
    "systemName": {
      "$id": "#/properties/systemName",
      "type": "string",
      "title": "The Systemname Schema",
      "examples": ["RequestAnimationFrameTimelineProvider"]
    }
  }
}
