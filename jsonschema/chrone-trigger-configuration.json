{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Chrono Trigger Configuration Schema",
    "required": [
      "id",
      "containerSelector",
      "language",
      "layoutTemplate",
      "availableLanguages",
      "timelines"
    ],
    "properties": {
      "id": {
        "$id": "#/properties/id",
        "type": "string",
        "title": "The Id Schema",
        "default": "",
        "examples": [
          "6291bf4c-07fb-49c6-9005-2dca98d1b3af"
        ],
        "pattern": "^(.*)$"
      },
      "playerSettings": {
        "$id": "#/properties/playerSettings",
        "type": "object",
        "title": "The Playersettings Schema",
        "required": [
          "vendor",
          "selector",
          "systemName",
          "playerApiKey"
        ],
        "properties": {
          "vendor": {
            "$id": "#/properties/playerSettings/properties/vendor",
            "type": "string",
            "title": "The Vendor Schema",
            "default": "",
            "examples": [
              "jwplayer"
            ],
            "pattern": "^(.*)$"
          },
          "selector": {
            "$id": "#/properties/playerSettings/properties/selector",
            "type": "string",
            "title": "The Selector Schema",
            "default": "",
            "examples": [
              "jwplayer"
            ],
            "pattern": "^(.*)$"
          },
          "systemName": {
            "$id": "#/properties/playerSettings/properties/systemName",
            "type": "string",
            "title": "The Systemname Schema",
            "default": "",
            "examples": [
              "JwPlayerFacade"
            ],
            "pattern": "^(.*)$"
          },
          "playerApiKey": {
            "$id": "#/properties/playerSettings/properties/playerApiKey",
            "type": "string",
            "title": "The Playerapikey Schema",
            "default": "",
            "examples": [
              "QRa1gzd9/l8kw5567O0lneCUr7L3woHSc/=="
            ],
            "pattern": "^(.*)$"
          }
        }
      },
      "containerSelector": {
        "$id": "#/properties/containerSelector",
        "type": "string",
        "title": "The Containerselector Schema",
        "default": "",
        "examples": [
          "#ivr-container"
        ],
        "pattern": "^(.*)$"
      },
      "language": {
        "$id": "#/properties/language",
        "type": "string",
        "title": "The Language Schema",
        "default": "",
        "examples": [
          "en-US"
        ],
        "pattern": "^(.*)$"
      },
      "layoutTemplate": {
        "$id": "#/properties/layoutTemplate",
        "type": "string",
        "title": "The Layouttemplate Schema",
        "default": "",
        "examples": [
          "template:layoutTemplate"
        ],
        "pattern": "^(.*)$"
      },
      "availableLanguages": {
        "$id": "#/properties/availableLanguages",
        "type": "array",
        "title": "The Availablelanguages Schema",
        "items": {
          "$id": "#/properties/availableLanguages/items",
          "type": "object",
          "title": "The Items Schema",
          "required": [
            "code",
            "label"
          ],
          "properties": {
            "code": {
              "$id": "#/properties/availableLanguages/items/properties/code",
              "type": "string",
              "title": "The Code Schema",
              "default": "",
              "examples": [
                "en-US"
              ],
              "pattern": "^(.*)$"
            },
            "label": {
              "$id": "#/properties/availableLanguages/items/properties/label",
              "type": "string",
              "title": "The Label Schema",
              "default": "",
              "examples": [
                "English"
              ],
              "pattern": "^(.*)$"
            }
          }
        }
      },
      "initActions": {
        "$id": "#/properties/initActions",
        "type": "array",
        "title": "The Initactions Schema",
        "items": {
          "$id": "#/properties/initActions/items",
          "type": "object",
          "title": "The Items Schema",
          "required": [
            "name",
            "startOperations",
            "endOperations"
          ],
          "properties": {
            "name": {
              "$id": "#/properties/initActions/items/properties/name",
              "type": "string",
              "title": "The Name Schema",
              "default": "",
              "examples": [
                "ProgressbarSetup"
              ],
              "pattern": "^(.*)$"
            },
            "startOperations": {
              "$id": "#/properties/initActions/items/properties/startOperations",
              "type": "array",
              "title": "The Startoperations Schema",
              "items": {
                "$id": "#/properties/initActions/items/properties/startOperations/items",
                "type": "object",
                "title": "The Items Schema",
                "required": [
                  "systemName",
                  "operationData"
                ],
                "properties": {
                  "systemName": {
                    "$id": "#/properties/initActions/items/properties/startOperations/items/properties/systemName",
                    "type": "string",
                    "title": "The Systemname Schema",
                    "default": "",
                    "examples": [
                      "SelectElementOperation"
                    ],
                    "pattern": "^(.*)$"
                  },
                  "operationData": {
                    "$id": "#/properties/initActions/items/properties/startOperations/items/properties/operationData",
                    "type": "object",
                    "title": "The Operationdata Schema",
                    "required": [
                      "selector"
                    ],
                    "properties": {
                      "selector": {
                        "$id": "#/properties/initActions/items/properties/startOperations/items/properties/operationData/properties/selector",
                        "type": "string",
                        "title": "The Selector Schema",
                        "default": "",
                        "examples": [
                          "#progress"
                        ],
                        "pattern": "^(.*)$"
                      }
                    }
                  }
                }
              }
            },
            "endOperations": {
              "$id": "#/properties/initActions/items/properties/endOperations",
              "type": "array",
              "title": "The Endoperations Schema",
              "items": {
                "$id": "#/properties/initActions/items/properties/endOperations/items",
                "type": "object",
                "title": "The Items Schema",
                "required": [
                  "systemName",
                  "operationData"
                ],
                "properties": {
                  "systemName": {
                    "$id": "#/properties/initActions/items/properties/endOperations/items/properties/systemName",
                    "type": "string",
                    "title": "The Systemname Schema",
                    "default": "",
                    "examples": [
                      "SelectElementOperation"
                    ],
                    "pattern": "^(.*)$"
                  },
                  "operationData": {
                    "$id": "#/properties/initActions/items/properties/endOperations/items/properties/operationData",
                    "type": "object",
                    "title": "The Operationdata Schema",
                    "required": [
                      "selector"
                    ],
                    "properties": {
                      "selector": {
                        "$id": "#/properties/initActions/items/properties/endOperations/items/properties/operationData/properties/selector",
                        "type": "string",
                        "title": "The Selector Schema",
                        "default": "",
                        "examples": [
                          "#progress"
                        ],
                        "pattern": "^(.*)$"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "actions": {
        "$id": "#/properties/actions",
        "type": "array",
        "title": "The Actions Schema",
        "items": {
          "$id": "#/properties/actions/items",
          "type": "object",
          "title": "The Items Schema",
          "required": [
            "name",
            "startOperations"
          ],
          "properties": {
            "name": {
              "$id": "#/properties/actions/items/properties/name",
              "type": "string",
              "title": "The Name Schema",
              "default": "",
              "examples": [
                "BroadcastLanguageChange"
              ],
              "pattern": "^(.*)$"
            },
            "startOperations": {
              "$id": "#/properties/actions/items/properties/startOperations",
              "type": "array",
              "title": "The Startoperations Schema",
              "items": {
                "$id": "#/properties/actions/items/properties/startOperations/items",
                "type": "object",
                "title": "The Items Schema",
                "required": [
                  "systemName",
                  "operationData"
                ],
                "properties": {
                  "systemName": {
                    "$id": "#/properties/actions/items/properties/startOperations/items/properties/systemName",
                    "type": "string",
                    "title": "The Systemname Schema",
                    "default": "",
                    "examples": [
                      "BroadcastEventOperation"
                    ],
                    "pattern": "^(.*)$"
                  },
                  "operationData": {
                    "$id": "#/properties/actions/items/properties/startOperations/items/properties/operationData",
                    "type": "object",
                    "title": "The Operationdata Schema",
                    "required": [
                      "eventName"
                    ],
                    "properties": {
                      "eventName": {
                        "$id": "#/properties/actions/items/properties/startOperations/items/properties/operationData/properties/eventName",
                        "type": "string",
                        "title": "The Eventname Schema",
                        "default": "",
                        "examples": [
                          "language-change"
                        ],
                        "pattern": "^(.*)$"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "timelines": {
        "$id": "#/properties/timelines",
        "type": "array",
        "title": "The Timelines Schema",
        "items": {
          "$id": "#/properties/timelines/items",
          "type": "object",
          "title": "The Items Schema",
          "required": [
            "type",
            "url",
            "loop",
            "timelineActions"
          ],
          "properties": {
            "type": {
              "$id": "#/properties/timelines/items/properties/type",
              "type": "string",
              "title": "The Type Schema",
              "default": "",
              "examples": [
                "video"
              ],
              "pattern": "^(.*)$"
            },
            "url": {
              "$id": "#/properties/timelines/items/properties/url",
              "type": "string",
              "title": "The Url Schema",
              "default": "",
              "examples": [
                "Q4FY_2013_EN [CROPPED 341x389]_1.mp4"
              ],
              "pattern": "^(.*)$"
            },
            "loop": {
              "$id": "#/properties/timelines/items/properties/loop",
              "type": "boolean",
              "title": "The Loop Schema",
              "default": false,
              "examples": [
                false
              ]
            },
            "timelineActions": {
              "$id": "#/properties/timelines/items/properties/timelineActions",
              "type": "array",
              "title": "The Timelineactions Schema",
              "items": {
                "$id": "#/properties/timelines/items/properties/timelineActions/items",
                "type": "object",
                "title": "The Items Schema",
                "required": [
                  "name",
                  "duration",
                  "startOperations",
                  "endOperations"
                ],
                "properties": {
                  "name": {
                    "$id": "#/properties/timelines/items/properties/timelineActions/items/properties/name",
                    "type": "string",
                    "title": "The Name Schema",
                    "default": "",
                    "examples": [
                      "ShowChartHistoryItem"
                    ],
                    "pattern": "^(.*)$"
                  },
                  "duration": {
                    "$id": "#/properties/timelines/items/properties/timelineActions/items/properties/duration",
                    "type": "object",
                    "title": "The Duration Schema",
                    "required": [
                      "start"
                    ],
                    "properties": {
                      "start": {
                        "$id": "#/properties/timelines/items/properties/timelineActions/items/properties/duration/properties/start",
                        "type": "integer",
                        "title": "The Start Schema",
                        "default": 0,
                        "examples": [
                          7
                        ]
                      }
                    }
                  },
                  "startOperations": {
                    "$id": "#/properties/timelines/items/properties/timelineActions/items/properties/startOperations",
                    "type": "array",
                    "title": "The Startoperations Schema",
                    "items": {
                      "$id": "#/properties/timelines/items/properties/timelineActions/items/properties/startOperations/items",
                      "type": "object",
                      "title": "The Items Schema",
                      "required": [
                        "systemName",
                        "operationData"
                      ],
                      "properties": {
                        "systemName": {
                          "$id": "#/properties/timelines/items/properties/timelineActions/items/properties/startOperations/items/properties/systemName",
                          "type": "string",
                          "title": "The Systemname Schema",
                          "default": "",
                          "examples": [
                            "SelectElementOperation"
                          ],
                          "pattern": "^(.*)$"
                        },
                        "operationData": {
                          "$id": "#/properties/timelines/items/properties/timelineActions/items/properties/startOperations/items/properties/operationData",
                          "type": "object",
                          "title": "The Operationdata Schema",
                          "required": [
                            "selector"
                          ],
                          "properties": {
                            "selector": {
                              "$id": "#/properties/timelines/items/properties/timelineActions/items/properties/startOperations/items/properties/operationData/properties/selector",
                              "type": "string",
                              "title": "The Selector Schema",
                              "default": "",
                              "examples": [
                                ".view-history-container"
                              ],
                              "pattern": "^(.*)$"
                            }
                          }
                        }
                      }
                    }
                  },
                  "endOperations": {
                    "$id": "#/properties/timelines/items/properties/timelineActions/items/properties/endOperations",
                    "type": "array",
                    "title": "The Endoperations Schema",
                    "items": {
                      "$id": "#/properties/timelines/items/properties/timelineActions/items/properties/endOperations/items",
                      "type": "object",
                      "title": "The Items Schema",
                      "required": [
                        "systemName",
                        "operationData"
                      ],
                      "properties": {
                        "systemName": {
                          "$id": "#/properties/timelines/items/properties/timelineActions/items/properties/endOperations/items/properties/systemName",
                          "type": "string",
                          "title": "The Systemname Schema",
                          "default": "",
                          "examples": [
                            "SelectElementOperation"
                          ],
                          "pattern": "^(.*)$"
                        },
                        "operationData": {
                          "$id": "#/properties/timelines/items/properties/timelineActions/items/properties/endOperations/items/properties/operationData",
                          "type": "object",
                          "title": "The Operationdata Schema",
                          "required": [
                            "selector"
                          ],
                          "properties": {
                            "selector": {
                              "$id": "#/properties/timelines/items/properties/timelineActions/items/properties/endOperations/items/properties/operationData/properties/selector",
                              "type": "string",
                              "title": "The Selector Schema",
                              "default": "",
                              "examples": [
                                "#view-history-item-1"
                              ],
                              "pattern": "^(.*)$"
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "eventActions": {
        "$id": "#/properties/eventActions",
        "type": "array",
        "title": "The Eventactions Schema"
      },
      "labels": {
        "$id": "#/properties/labels",
        "type": "array",
        "title": "The Labels Schema"
      }
    }
  }