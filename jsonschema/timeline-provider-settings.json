{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://rolandzwaga.github.io/eligius/jsonschema/timeline-provider-settings.json",
  "type": "object",
  "title": "The TimelineProviderSettings Schema",
  "required": ["selector", "systemName"],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "examples": ["123e4567-e89b-12d3-a456-426614174000"],
      "pattern": "^[0-9a-fA-F]{8}\b-[0-9a-fA-F]{4}\b-[0-9a-fA-F]{4}\b-[0-9a-fA-F]{4}\b-[0-9a-fA-F]{12}$"
    },
    "vendor": {
      "$id": "#/properties/vendor",
      "type": "string",
      "title": "The Vendor Schema",
      "examples": ["jwplayer"]
    },
    "selector": {
      "$id": "#/properties/selector",
      "type": "string",
      "title": "The Selector Schema"
    },
    "systemName": {
      "$id": "#/properties/systemName",
      "type": "string",
      "title": "The Systemname Schema",
      "examples": ["JwPlayerTimelineProvider"]
    },
    "playerApiKey": {
      "$id": "#/properties/playerApiKey",
      "type": "string",
      "title": "The Playerapikey Schema",
      "examples": ["QRa1gzd9/l8kw5567O0lneCUr7L3woHSc/=="]
    }
  }
}
