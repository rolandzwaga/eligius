{
  "version": "1.0.5",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/eligius.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "dev": "dts watch",
    "build": "dts build",
    "test": "cross-env TS_NODE_PROJECT=tsconfig.test.json uvu -r ts-node/register -r jsdom-global/register src/test",
    "check": "tsc --pretty --noEmit",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "generate-schema": "ts-node ./src/schema-tools/generate-operation-schemas",
    "typedoc": "typedoc --options typedoc.json",
    "docs": "yarn generate-schema && yarn typedoc && node copy-schema"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "eligius",
  "author": "rolandzwaga",
  "size-limit": [
    {
      "path": "dist/eligius.cjs.production.min.js",
      "limit": "150 KB"
    },
    {
      "path": "dist/eligius.esm.js",
      "limit": "150 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "8.1.0",
    "@types/chai": "4.3.3",
    "@types/fs-extra": "9.0.13",
    "@types/jasmine": "4.3.0",
    "@types/jquery": "3.5.14",
    "@types/jsdom": "^20.0.0",
    "@types/mousetrap": "1.6.9",
    "@types/node": "18.11.3",
    "@types/sinon": "10.0.13",
    "@types/uuid": "8.3.4",
    "acorn": "8.8.0",
    "ajv": "8.11.0",
    "astring": "1.8.3",
    "babel-jest": "29.2.1",
    "canvas": "2.10.1",
    "chai": "4.3.6",
    "cross-env": "7.0.3",
    "dts-cli": "1.6.0",
    "fs-extra": "10.1.0",
    "husky": "8.0.1",
    "jquery": "3.6.1",
    "jsdom": "20.0.1",
    "jsdom-global": "3.0.2",
    "lottie-web": "5.9.6",
    "mediaelement": "5.1.0",
    "sinon": "14.0.1",
    "size-limit": "8.1.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tslib": "2.4.0",
    "typedoc": "0.23.17",
    "typedoc-plugin-missing-exports": "1.0.0",
    "typescript": "4.8.4",
    "uvu": "0.5.6"
  },
  "peerDependencies": {
    "jquery": "3.6.0",
    "lottie-web": "5.8.1",
    "mediaelement": "5.0.4"
  },
  "dependencies": {
    "mousetrap": "1.6.5",
    "ts-is-present": "1.2.2",
    "uuid": "9.0.0"
  },
  "packageManager": "yarn@3.2.4"
}
