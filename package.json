{
  "name": "eligius",
  "author": "Roland Zwaga <rbzwaga@gmail.com>",
  "version": "1.5.1",
  "license": "MIT",
  "homepage": "https://rolandzwaga.github.io/eligius/",
  "bugs": {
    "url": "https://github.com/rolandzwaga/eligius/issues?q=is%3Aissue+is%3Aopen+label%3ABug"
  },
  "type": "module",
  "module": "./dist/index.mjs",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "jsonschema"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "typecheck": "tsc --pretty --noEmit",
    "test": "vitest --config src/vitest.config.ts --no-file-parallelism",
    "test:ci": "vitest run --config src/vitest.config.ts --reporter=verbose",
    "coverage": "vitest --config src/vitest.config.ts --coverage",
    "generate-readmes": "tsx ./src/tools/doc-tools/generate-readmes",
    "generate-schema": "tsx ./src/tools/schema-tools/generate-operation-schemas",
    "typedoc": "typedoc --options typedoc-metadata.json & typedoc --options typedoc.json",
    "docs": "npm run generate-schema && npm run typedoc && node copy-schema && npm run generate-readmes",
    "opers-metadata": "tsx src/tools/code-generator/metadata-generator.ts",
    "ctrls-metadata": "tsx src/tools/code-generator/ctrls-metadata-generator.ts",
    "events-metadata": "tsx src/tools/code-generator/events-metadata-generator.ts",
    "metadata": "npm run opers-metadata && npm run ctrls-metadata && npm run events-metadata",
    "ts-types": " tsc --noEmit false --emitDeclarationOnly --outDir dist",
    "build": "rimraf dist && npm run metadata && npm run generate-schema && tsdown",
    "codemod": "tsx src/tools/code-generator/uvu2vitest-codemod.ts",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "fix": "biome check --write ."
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.4",
    "@ts-morph/bootstrap": "0.28.1",
    "@types/chai": "5.2.3",
    "@types/fs-extra": "11.0.4",
    "@types/jquery": "3.5.33",
    "@types/jsdom": "27.0.0",
    "@types/node": "24.10.1",
    "@types/sinon": "17.0.4",
    "@types/uuid": "11.0.0",
    "@vitest/coverage-istanbul": "4.0.8",
    "canvas": "3.2.0",
    "chai": "6.2.0",
    "fs-extra": "11.3.2",
    "jquery": "3.7.1",
    "jsdom": "27.2.0",
    "jsdom-global": "3.0.2",
    "lottie-web": "5.13.0",
    "rimraf": "6.1.0",
    "sinon": "21.0.0",
    "ts-morph": "27.0.2",
    "tsconfig-paths": "4.2.0",
    "tsdown": "0.16.5",
    "tslib": "2.8.1",
    "tsx": "4.20.6",
    "typedoc": "0.28.14",
    "typedoc-plugin-missing-exports": "4.1.2",
    "typescript": "5.9.3",
    "video.js": "8.23.4",
    "vitest": "4.0.8",
    "watchlist": "0.3.1"
  },
  "peerDependencies": {
    "jquery": "3.7.1",
    "lottie-web": "5.13.0",
    "video.js": "8.23.4"
  },
  "dependencies": {
    "hotkeys-js": "3.13.15",
    "ts-is-present": "1.2.2",
    "uuid": "13.0.0"
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts"
}
