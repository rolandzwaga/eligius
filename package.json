{
  "name": "eligius",
  "author": "Roland Zwaga <rbzwaga@gmail.com>",
  "version": "1.1.4",
  "license": "MIT",
  "homepage": "https://rolandzwaga.github.io/eligius/",
  "bugs": {
    "url": "https://github.com/rolandzwaga/eligius/issues?q=is%3Aissue+is%3Aopen+label%3ABug"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./metadata": {
      "types": "./dist/operation/metadata/types.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "typecheck": "tsc --pretty --noEmit",
    "test": "cross-env TS_NODE_PROJECT=tsconfig.test.json uvu -r ts-node/register -r jsdom-global/register src/test",
    "test:watch": "watchlist src test -- npm run test",
    "prepare": "npm run build",
    "generate-readmes": "ts-node ./src/tools/doc-tools/generate-readmes",
    "generate-schema": "ts-node ./src/tools/schema-tools/generate-operation-schemas",
    "typedoc": "typedoc --options typedoc-metadata.json & typedoc --options typedoc.json",
    "docs": "npm run generate-schema && npm run typedoc && node copy-schema && npm run generate-readmes",
    "ts-types": " tsc --emitDeclarationOnly --outDir dist",
    "build": "rimraf dist && node ./esbuild.js & npm run ts-types"
  },
  "devDependencies": {
    "@ts-morph/bootstrap": "0.24.0",
    "@types/chai": "4.3.19",
    "@types/fs-extra": "11.0.4",
    "@types/jquery": "3.5.30",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.5.5",
    "@types/sinon": "17.0.3",
    "@types/uuid": "10.0.0",
    "canvas": "2.11.2",
    "chai": "4.4.1",
    "cross-env": "7.0.3",
    "esbuild": "0.23.1",
    "fs-extra": "11.2.0",
    "jquery": "3.7.1",
    "jsdom": "25.0.0",
    "jsdom-global": "3.0.2",
    "lottie-web": "5.12.2",
    "rimraf": "6.0.1",
    "sinon": "19.0.2",
    "ts-morph": "23.0.0",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.7.0",
    "typedoc": "0.26.7",
    "typedoc-plugin-missing-exports": "3.0.0",
    "typescript": "5.6.2",
    "uuid": "10.0.0",
    "uvu": "0.5.6",
    "video.js": "8.17.4",
    "vitest": "2.1.1",
    "watchlist": "0.3.1"
  },
  "peerDependencies": {
    "jquery": "3.7.1",
    "lottie-web": "5.11.0",
    "uuid": "9.0.0",
    "video.js": "7.21.0"
  },
  "dependencies": {
    "hotkeys-js": "3.13.7",
    "ts-is-present": "1.2.2"
  }
}
