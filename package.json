{
  "name": "eligius",
  "author": "Roland Zwaga <rbzwaga@gmail.com>",
  "version": "1.1.4",
  "license": "MIT",
  "homepage": "https://rolandzwaga.github.io/eligius/",
  "bugs": {
    "url": "https://github.com/rolandzwaga/eligius/issues?q=is%3Aissue+is%3Aopen+label%3ABug"
  },
  "type": "module",
  "module": "dist/index.esm.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js"
    },
    "./metadata": {
      "types": "./dist/operation/metadata/types.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*"
      ]
    }
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "typecheck": "tsc --pretty --noEmit",
    "test": "vitest --config src/vitest.config.ts",
    "coverage": "vitest --config src/vitest.config.ts --coverage",
    "prepare": "npm run build",
    "generate-readmes": "tsx ./src/tools/doc-tools/generate-readmes",
    "generate-schema": "tsx ./src/tools/schema-tools/generate-operation-schemas",
    "typedoc": "typedoc --options typedoc-metadata.json & typedoc --options typedoc.json",
    "docs": "npm run generate-schema && npm run typedoc && node copy-schema && npm run generate-readmes",
    "opers-metadata": "tsx src/tools/code-generator/metadata-generator.ts",
    "ctrls-metadata": "tsx src/tools/code-generator/ctrls-metadata-generator.ts",
    "metadata": "npm run opers-metadata && npm run ctrls-metadata",
    "ts-types": " tsc --noEmit false --emitDeclarationOnly --outDir dist",
    "build": "rimraf dist && npm run metadata && node ./esbuild.cjs & npm run ts-types",
    "codemod": "tsx src/tools/code-generator/uvu2vitest-codemod.ts"
  },
  "devDependencies": {
    "@ts-morph/bootstrap": "0.26.1",
    "@types/chai": "5.0.1",
    "@types/fs-extra": "11.0.4",
    "@types/jquery": "3.5.32",
    "@types/jsdom": "21.1.7",
    "@types/node": "22.13.4",
    "@types/sinon": "17.0.3",
    "@types/uuid": "10.0.0",
    "canvas": "3.1.0",
    "chai": "5.2.0",
    "esbuild": "0.25.0",
    "fs-extra": "11.3.0",
    "jquery": "3.7.1",
    "jsdom": "26.0.0",
    "jsdom-global": "3.0.2",
    "lottie-web": "5.12.2",
    "rimraf": "6.0.1",
    "sinon": "19.0.2",
    "ts-morph": "25.0.1",
    "tsconfig-paths": "4.2.0",
    "tslib": "2.8.1",
    "tsx": "4.19.3",
    "typedoc": "0.27.7",
    "typedoc-plugin-missing-exports": "3.1.0",
    "typescript": "5.7.3",
    "uuid": "11.0.5",
    "uvu": "0.5.6",
    "video.js": "8.21.0",
    "vitest": "3.0.6",
    "watchlist": "0.3.1"
  },
  "peerDependencies": {
    "jquery": "3.7.1",
    "lottie-web": "5.11.0",
    "uuid": "9.0.0",
    "video.js": "8.21.0"
  },
  "dependencies": {
    "hotkeys-js": "3.13.9",
    "ts-is-present": "1.2.2"
  }
}
